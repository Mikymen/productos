{
  "version": 2,
  "builds": [
	  { "src": "/public_html/index.php", "use": "vercel-php@0.3.3" },
	  { "src": "/public_html/assets/**", "use": "@vercel/static" }
  ],
  "rewrites": [
        {
            "source": "/(assets)/(.*)",
            "destination": "public_html/$1/$2"
        },
        {
            "source": "/(.*)",
            "destination": "/public_html/index.php"
        }
    ],
    
    "redirects":[
        {
            "source": "/",
            "destination": "/public_html/"
        }
    ]

}